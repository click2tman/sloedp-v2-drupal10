{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/address": "^1.11",
        "drupal/admin_toolbar": "^3.3",
        "drupal/aggregator": "^2.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/autocomplete_deluxe": "^2.0",
        "drupal/autologout": "^1.4",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_class": "^2.0",
        "drupal/bootstrap_layout_builder": "^2.1",
        "drupal/bootstrap_styles": "^1.1",
        "drupal/calendar_view": "^2.0",
        "drupal/captcha": "^1.9",
        "drupal/charts": "^5.0",
        "drupal/checklistapi": "^2.1",
        "drupal/clientside_validation": "^4.0",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_rewrite": "^1.5",
        "drupal/config_update": "^2.0@alpha",
        "drupal/consumers": "^1.17",
        "drupal/contact_block": "^2.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/crop": "^2.3",
        "drupal/csv_importer": "^1.16",
        "drupal/ctools": "^4.0",
        "drupal/depcalc": "^1.16",
        "drupal/devel": "^5.1",
        "drupal/diff": "^1.1",
        "drupal/dropzonejs": "^2.8",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/facets": "^3.0@beta",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/file_mdm": "^2.5",
        "drupal/focal_point": "^2.0@alpha",
        "drupal/geocoder": "^4.5",
        "drupal/geofield": "^1.52",
        "drupal/geofield_map": "^3.0",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^2.0@beta",
        "drupal/honeypot": "^2.1",
        "drupal/imagemagick": "^3.4",
        "drupal/imce": "^3.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui": "^1.6",
        "drupal/jquery_ui_draggable": "^2.0",
        "drupal/jsonapi_extras": "^3.23",
        "drupal/jsonapi_menu_items": "^1.2",
        "drupal/jsonapi_resources": "^1.0@beta",
        "drupal/layout_builder_blocks": "^1.1",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/leaflet": "^10.0",
        "drupal/libraries": "^4.0",
        "drupal/ludwig": "^2.0",
        "drupal/memcache": "^2.5",
        "drupal/metatag": "^1.22",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/migrate_spreadsheet": "^2.1",
        "drupal/migrate_tools": "^6.0",
        "drupal/migrate_upgrade": "^4.0",
        "drupal/moderation_dashboard": "^2.1",
        "drupal/moderation_sidebar": "^1.7",
        "drupal/openai": "^1.0@alpha",
        "drupal/openapi": "^2.1",
        "drupal/paragraphs": "^1.15",
        "drupal/password_policy": "^4.0",
        "drupal/pathauto": "^1.11",
        "drupal/profile": "^1.7",
        "drupal/purge": "^3.4",
        "drupal/real_aes": "^2.5",
        "drupal/recaptcha": "^3.1",
        "drupal/redirect": "^1.8",
        "drupal/remove_http_headers": "^2.0",
        "drupal/reroute_email": "^2.2",
        "drupal/responsive_preview": "^2.1",
        "drupal/scheduler": "^2.0@RC",
        "drupal/schemata": "^1.0@beta",
        "drupal/search_api": "^1.29",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/seckit": "^2.0",
        "drupal/select2": "^1.14",
        "drupal/shield": "^1.7",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_trim": "^2.0",
        "drupal/social_media_links": "^2.9",
        "drupal/state_machine": "^1.7",
        "drupal/storage": "^1.2",
        "drupal/subrequests": "^3.0",
        "drupal/tfa": "^2.0@alpha",
        "drupal/token": "^1.11",
        "drupal/tvi": "^2.0@alpha",
        "drupal/twitter_block": "^3.0",
        "drupal/username_enumeration_prevention": "^1.3",
        "drupal/webform": "^6.2@beta",
        "drupal/webprofiler": "^10.0",
        "drupal/workbench_email": "^3.0",
        "drupal/workbench_moderation": "^1.7",
        "drush/drush": "^11.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
